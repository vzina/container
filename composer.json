{
    "name": "open-ef/container",
    "description": "php aop",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "OpenEf\\Container\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenEfTest\\": "tests"
        }
    },
    "authors": [
        {
            "name": "weijian.ye",
            "email": "yeweijian299@163.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.2",
        "ext-pcntl": "*",
        "illuminate/collections": "^8.0",
        "illuminate/pipeline": "^8.83",
        "nette/php-generator": "^4.1",
        "nikic/php-parser": "^5.0",
        "php-di/phpdoc-reader": "^2.2",
        "pimple/pimple": "^3.0",
        "psr/container": "^1.0",
        "symfony/finder": "^5.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": ">=7.0"
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-autoload-dump": [
            "rm -rf runtime/container"
        ],
        "test": "phpunit --prepend test/bootstrap.php -c phpunit.xml --colors=always",
        "cs-fix": "php-cs-fixer fix $1",
        "analyse": "phpstan analyse --memory-limit 300M -l 0 -c phpstan.neon ./app ./config"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "open_ef": {
            "config": "OpenEf\\Container\\ComponentProvider"
        }
    }
}
